# foi configurado o git

aluno@aluno-pc:~$ git config --global user.name "Felipe Ramos"
aluno@aluno-pc:~$ git config --global user.email "felipe.pi@outlook.com"

aluno@aluno-pc:~$ mkdir workspace #mkdir para criar as pastas de tabalho
aluno@aluno-pc:~$ cd workspace
aluno@aluno-pc:~/workspace$ mkdir poo
aluno@aluno-pc:~/workspace$ cd poo
aluno@aluno-pc:~/workspace/poo$ 

